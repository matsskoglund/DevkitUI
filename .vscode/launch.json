{
    "version": "0.2.0",
    "configurations": 
    [{
        "name": "Attach to Chrome, with sourcemaps",
        "type": "chrome",
        "request": "attach",
        "url": "http://localhost:4200",
        "port": 9222,
        "sourceMaps": true,
        "trace": true,
        "webRoot": "${workspaceRoot}"
    },
    {
        "name": "Launch localhost",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceRoot}"
    }]
}